<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#ffffff"
    tools:context=".ui.fragment.StoryPathFragment">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/bot_image1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="30dp"
        android:layout_marginTop="10dp"
        android:visibility="visible"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/top_bar_bot_inner_image_container"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@drawable/circle_backgroun"
            android:padding="20dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent">

            <ImageView
                android:id="@+id/top_bar_bot_image"
                android:layout_width="70dp"
                android:layout_height="70dp"
                android:src="@drawable/taxi1"
                android:visibility="visible"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent" />

        </androidx.constraintlayout.widget.ConstraintLayout>

        <TextView
            android:id="@+id/top_bar_title_desc"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:fontFamily="@font/avenir_next"
            android:gravity="center"
            android:text="Taxi Pete"
            android:textColor="#000000"
            android:textSize="25sp"
            android:textStyle="bold"
            app:layout_constraintEnd_toEndOf="@id/top_bar_bot_inner_image_container"
            app:layout_constraintStart_toStartOf="@id/top_bar_bot_inner_image_container"
            app:layout_constraintTop_toBottomOf="@id/top_bar_bot_inner_image_container" />

    </androidx.constraintlayout.widget.ConstraintLayout>

    <ImageView
        android:id="@+id/line1"
        android:layout_width="200dp"
        android:layout_height="90dp"
        android:layout_marginStart="110dp"
        android:layout_marginBottom="20dp"
        android:src="@drawable/path_line_curve_rounded"
        app:layout_constraintBottom_toBottomOf="@id/bot_image1"
        app:layout_constraintStart_toStartOf="@id/bot_image1" />

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/bot_image2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="50dp"
        android:layout_marginTop="10dp"
        android:visibility="visible"
        app:layout_constraintStart_toStartOf="@id/line1"
        app:layout_constraintTop_toBottomOf="@id/line1">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/top_bar_bot_inner_image_container2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@drawable/circle_backgroun"
            android:padding="20dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent">

            <ImageView
                android:id="@+id/top_bar_bot_image2"
                android:layout_width="70dp"
                android:layout_height="70dp"
                android:src="@drawable/doctor_image"
                android:visibility="visible"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent" />

        </androidx.constraintlayout.widget.ConstraintLayout>

        <TextView
            android:id="@+id/top_bar_title_desc2"
            android:layout_width="150dp"
            android:layout_height="wrap_content"
            android:fontFamily="@font/avenir_next"
            android:gravity="center"
            android:text="Dr. Hum-Vee"
            android:textColor="#000000"
            android:textSize="25sp"
            android:textStyle="bold"
            app:layout_constraintEnd_toEndOf="@id/top_bar_bot_inner_image_container2"
            app:layout_constraintStart_toStartOf="@id/top_bar_bot_inner_image_container2"
            app:layout_constraintTop_toBottomOf="@id/top_bar_bot_inner_image_container2" />

    </androidx.constraintlayout.widget.ConstraintLayout>

    <TextView
        android:id="@+id/dot1"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:layout_marginStart="50dp"
        android:layout_marginBottom="60dp"
        android:background="@drawable/grey_circle_filled"
        app:layout_constraintBottom_toBottomOf="@id/bot_image1"
        app:layout_constraintStart_toStartOf="@id/line1"
        app:layout_constraintTop_toTopOf="@id/bot_image1" />

    <TextView
        android:id="@+id/dot2"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:layout_marginStart="40dp"
        android:layout_marginBottom="50dp"
        android:background="@drawable/grey_circle_filled"
        app:layout_constraintBottom_toBottomOf="@id/bot_image1"
        app:layout_constraintStart_toEndOf="@id/dot1"
        app:layout_constraintTop_toTopOf="@id/bot_image1" />

    <TextView
        android:id="@+id/dot3"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:layout_marginStart="20dp"
        android:layout_marginBottom="30dp"
        android:background="@drawable/grey_circle_filled"
        app:layout_constraintBottom_toTopOf="@id/bot_image2"
        app:layout_constraintStart_toStartOf="@id/dot2" />


    <ImageView
        android:id="@+id/line2"
        android:layout_width="200dp"
        android:layout_height="150dp"
        android:layout_marginTop="50dp"
        android:layout_marginEnd="130dp"
        android:src="@drawable/curve_line_left"
        app:layout_constraintEnd_toEndOf="@id/bot_image2"
        app:layout_constraintTop_toTopOf="@id/bot_image2" />

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/bot_image3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="30dp"
        android:layout_marginTop="10dp"
        android:visibility="visible"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/line2">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/top_bar_bot_inner_image_container3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@drawable/circle_backgroun"
            android:padding="20dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent">

            <ImageView
                android:id="@+id/top_bar_bot_image3"
                android:layout_width="70dp"
                android:layout_height="70dp"
                android:src="@drawable/policeman"
                android:visibility="visible"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent" />

        </androidx.constraintlayout.widget.ConstraintLayout>

        <TextView
            android:id="@+id/top_bar_title_desc3"
            android:layout_width="150dp"
            android:layout_height="wrap_content"
            android:fontFamily="@font/avenir_next"
            android:gravity="center"
            android:text="Policeman Paul"
            android:textColor="#000000"
            android:textSize="25sp"
            android:textStyle="bold"
            app:layout_constraintEnd_toEndOf="@id/top_bar_bot_inner_image_container3"
            app:layout_constraintStart_toStartOf="@id/top_bar_bot_inner_image_container3"
            app:layout_constraintTop_toBottomOf="@id/top_bar_bot_inner_image_container3" />

    </androidx.constraintlayout.widget.ConstraintLayout>

    <TextView
        android:id="@+id/dot1_image2"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:layout_marginEnd="50dp"
        android:layout_marginBottom="42dp"
        android:background="@drawable/grey_circle_filled"
        app:layout_constraintBottom_toBottomOf="@id/bot_image2"
        app:layout_constraintEnd_toEndOf="@id/line2"
        app:layout_constraintTop_toTopOf="@id/bot_image2" />

    <TextView
        android:id="@+id/dot2_image2"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:layout_marginEnd="50dp"
        android:background="@drawable/grey_circle_filled"
        app:layout_constraintEnd_toStartOf="@id/dot1_image2"
        app:layout_constraintTop_toBottomOf="@id/dot1_image2" />

    <TextView
        android:id="@+id/dot3_image2"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:layout_marginStart="25dp"
        android:layout_marginTop="50dp"
        android:background="@drawable/grey_circle_filled"
        app:layout_constraintStart_toStartOf="@id/line2"
        app:layout_constraintTop_toBottomOf="@id/dot2_image2" />

    <ImageView
        android:id="@+id/line3"
        android:layout_width="200dp"
        android:layout_height="90dp"
        android:layout_marginStart="130dp"
        android:layout_marginTop="50dp"
        android:src="@drawable/path_line_curve_rounded"
        app:layout_constraintStart_toStartOf="@id/bot_image3"
        app:layout_constraintTop_toTopOf="@id/bot_image3" />

    <TextView
        android:id="@+id/dot1_image3"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:layout_marginStart="50dp"
        android:layout_marginEnd="50dp"
        android:layout_marginTop="40dp"
        android:background="@drawable/grey_circle_filled"
        app:layout_constraintStart_toStartOf="@id/line3"
        app:layout_constraintTop_toTopOf="@id/bot_image3"

        />

    <TextView
        android:id="@+id/dot2_image3"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:layout_marginStart="60dp"
        android:background="@drawable/grey_circle_filled"
        app:layout_constraintStart_toStartOf="@id/dot1_image3"
        app:layout_constraintTop_toTopOf="@id/dot1_image3" />

    <TextView
        android:id="@+id/dot3_image3"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:layout_marginStart="40dp"
        android:layout_marginBottom="30dp"
        android:background="@drawable/grey_circle_filled"
        app:layout_constraintStart_toStartOf="@id/dot2_image3"
        app:layout_constraintBottom_toTopOf="@id/bot_image4"/>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/bot_image4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="80dp"
        android:layout_marginTop="10dp"
        android:visibility="visible"
        app:layout_constraintStart_toStartOf="@id/line3"
        app:layout_constraintTop_toBottomOf="@id/line3">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/top_bar_bot_inner_image_container4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@drawable/circle_backgroun"
            android:padding="20dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent">

            <ImageView
                android:id="@+id/top_bar_bot_image4"
                android:layout_width="70dp"
                android:layout_height="70dp"
                android:src="@drawable/aunt"
                android:visibility="visible"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent" />

        </androidx.constraintlayout.widget.ConstraintLayout>

        <TextView
            android:id="@+id/top_bar_title_desc4"
            android:layout_width="150dp"
            android:layout_height="wrap_content"
            android:fontFamily="@font/avenir_next"
            android:gravity="center"
            android:text="Aunt Annie"
            android:textColor="#000000"
            android:textSize="25sp"
            android:textStyle="bold"
            app:layout_constraintEnd_toEndOf="@id/top_bar_bot_inner_image_container4"
            app:layout_constraintStart_toStartOf="@id/top_bar_bot_inner_image_container4"
            app:layout_constraintTop_toBottomOf="@id/top_bar_bot_inner_image_container4" />

    </androidx.constraintlayout.widget.ConstraintLayout>

</androidx.constraintlayout.widget.ConstraintLayout>